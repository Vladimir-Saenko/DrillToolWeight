        $('.blog__link').on('click', function () {
            dataLayer.push({
                'event':'OWOX',
                'eventCategory': 'Interactions',
                'eventAction':'click',
                'eventLabel': 'Blog',
                'eventContext': null,
                'eventContent': null,
                'eventPosition': null,
                'eventLocation': null,
                'eventPortalName': null,
                'eventCategoryName': null,
                'eventCategoryId': null,
                'eventProductName': null,
                'eventProductId': null,
                'eventProductPrice': null,
                'ecommerce': null
            });
        });
        
		(function() {
				var values = {
					traders : 0,
					orders : 0
				};

				var comma_separator_number_step = $.animateNumber.numberStepFactories.separator(' ')

                var worker = new Worker('/js/worker/counter-worker.js?v=2');
                worker.onmessage = function (event) {
                    var data = event.data;
                    
                    val1 = parseInt(data.deals, 10), val2 = parseInt(data.traders, 10);

                    if(val1 !== values.traders) {
                        $('.counter-traders')
                        .attr('number', values.traders)
                        .animateNumber({
                            number: val1,
                            numberStep: comma_separator_number_step
                        });
                        values.traders = val1;
                    }

                    if(val2 !== values.orders) {
                        $('.counter-orders')
                        .attr('number', values.orders)
                        .animateNumber({
                            number: val2,
                            numberStep: comma_separator_number_step
                        });
                        values.orders = val2;
                    }
                };
                worker.postMessage('/site/counters');
		})();
