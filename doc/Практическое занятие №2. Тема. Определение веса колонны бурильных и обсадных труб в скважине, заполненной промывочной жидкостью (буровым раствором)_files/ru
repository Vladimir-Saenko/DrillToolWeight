        leoLangs = ["ru","tr","pt","es","es_LA","en","fr","it","de","zh_CN","ja","pl","cs","el","ko","nl","uk","hi_IN","vi","id","sr"]
        function reWriteLinks() {
            var path = window.location.pathname;
            var urgentSearchParams = 'targetLang';
            var searchParams = window.location.search;
            var pathSearch = searchParams.includes(urgentSearchParams) ? searchParams : '';
            var parsedPath = path.split('/');
            var domainName = window.location.origin;
            var currentLang = (parsedPath.length > 1 && leoLangs.includes(parsedPath[1])) ? parsedPath[1] : '';
            var prePath = parsedPath;
            var link = document.createElement('link');
            link.rel='canonical';
            link.href=domainName+path+searchParams;
            link.setAttribute('data-react-helmet', true);
            document.head.appendChild(link);
            leoLangs.map(lang => {
                prePath[1] = lang;
                const customLangs = {'es_LA': 'es', 'es': 'es-es', 'zh_CN': 'zh', 'hi_IN': 'hi'};
                const modLang = customLangs[lang] || lang;
                var langPath  = prePath.join('/');
                var url = domainName + langPath + searchParams;
                var link = document.createElement('link');
                link.rel='alternate';
                link.href=url;
                link.hreflang=modLang;
                link.setAttribute('data-react-helmet', true);
                document.head.appendChild(link);
            })            
        }
        reWriteLinks();
    </script>
 
    
