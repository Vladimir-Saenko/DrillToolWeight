<script>
	function initFinamHeader() {
		var model = {
			path: '/cache/header/finam/'
		};

		Finam.model = model;
		Finam.currentUserInfoinit();
		Finam.User.init(Finam.currentUserInfo);
		var header = new FinamHeader(model);
		header.init(document.querySelector('.fhr'));
	}

	document.addEventListener("DOMContentLoaded", initFinamHeader);
</script>

