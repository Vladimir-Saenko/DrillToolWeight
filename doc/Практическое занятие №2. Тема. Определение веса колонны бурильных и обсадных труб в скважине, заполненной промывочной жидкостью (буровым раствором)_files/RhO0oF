    <script type="text/javascript">
        $(document).ready(function() {

            // = Вставляем блок с PDF, если необходимо
            showPdfBlock();

            // Клик по картинке сертификата
            var ssImg = $('#site_seal_img');
            $(ssImg).mouseenter(function() {
                $(this).css({
                    'cursor': 'pointer'
                });
            });

            $(ssImg).css({
                'margin-top': '20px'
            });

            $(ssImg).click(function() {
                ss_open_sub();
            });

            //   Смена значка в таблице с деталями тарифов
            $(".tableHeader").click(function() {
                $(this).find("i").toggleClass("fa-minus fa-plus");
            });

            // scrollTopButton -s
            $(window).scroll(function() {
                if ($(this).scrollTop() > 100) {
                    $('.scrollup').fadeIn();
                } else {
                    $('.scrollup').fadeOut();
                }
            });

            $('.scrollup').click(function(event) {
                event.preventDefault();
                $("html, body").animate({
                    scrollTop: 0
                }, 600);
            });
        });
    </script>

