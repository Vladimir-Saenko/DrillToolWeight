            $("a[href='" + current_url + "']").addClass("active");

            // формирование таблицы с телефонами в шапке -s
            jQuery(function($) {
                $('#phone_table').footable({
                    "filtering": {
                        "enabled": false
                    }
                });
            });
        });
    </script>
