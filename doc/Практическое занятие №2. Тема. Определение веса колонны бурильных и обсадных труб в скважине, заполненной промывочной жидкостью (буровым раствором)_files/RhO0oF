    <script>
        $(document).ready(function() {

            // == поиск доменных имён на странице "Домены"
            function searchDomNames(domName) {
                if (!domName) {
                    // = просто пересылаем на страницу с доменами
                    window.location.href = "/services/domain";
                } else {
                    if (domName.indexOf('.') == -1) {
                        var zonesStr = '';
                        var enNameRegExp = new RegExp(/^[a-zA-Zîâășț0-9\-]+$/);
                        var ruNameRegExp = new RegExp(/^[а-яА-ЯёЁ0-9\-]+$/);
                        // = если слово корректное
                        if (enNameRegExp.test(domName) || ruNameRegExp.test(domName)) {
                            zonesStr = 'try_default';
                        }
                        // = нет точки
                        window.location.href = "/services/domain?zone=" + zonesStr + "&name=" + domName;
                    } else {
                        // = есть точка
                        // var nameDom = domName.split(".")[0];
                        // var nameZone = domName.split(".")[1];

                        var arrForSearch = domName.split(".");

                        var nameDom = arrForSearch[0];
                        var nameZone = arrForSearch[arrForSearch.length - 1];

                        if (arrForSearch.length > 2) {
                            arrForSearch.pop();
                            nameDom = arrForSearch.join('.');
                        }

                        window.location.href = "/services/domain?zone=" + nameZone + "&name=" + nameDom;
                    }
                }
            }

            // == Переход по псевдо-ссылкам для поиска доменов и whois
            function showDomInfo(event) {
                // = значение, которое пользователь ввёл в инпут
                var domName = $('.dom_name_inp_header').val();
                // = проверяем по какому элементу кликнули
                var targ = $(event.target)[0].id;

                // == controllers
                // = {click}
                if (event.type == 'click') {
                    switch (targ) {
                        // == домены
                        case 'domSubmitHeader':
                            searchDomNames(domName)
                            break;
                            // == whois
                        case 'whoisSubmitHeader':
                            if (!domName) {
                                // = просто пересылаем на страницу с whois
                                window.location.href = "/whois/check/domain";
                            } else {
                                window.location.href = "/whois/check/domain/" + domName;
                            }
                            break;
                    }
                }

                // = {enter}
                if (event.type == 'keydown' && event.keyCode == 13) {
                    searchDomNames(domName)
                }
            }

            document.getElementById('domInputHolderHeader').addEventListener('click', showDomInfo);
            document.getElementById('domInputHolderHeader').addEventListener('keydown', showDomInfo);

            // = Изменить стиль элемента в футере
            setTimeout(() => {
                $('.footer_menu_holder .chosen-container-single').css({
                    'width': '100%'
                });
            }, 3000);

            // псевдо-ссылка на экспресс оплату
            // expressPayFooter.addEventListener('click', function() {
            //     window.open(expressPayFooter.dataset.link);
            //     return false;
            // });

            // подгрузка текущего года
            function getCurrentYear(id) {
                var date = new Date();
                date = date.getFullYear();
                var identifier = "#" + id;
                $(identifier).html(date);
            }

            getCurrentYear('currentYear');

            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                $(".phones_pseudo_outer").css({
                    display: 'none'
                });
                $(".phone").css({
                    display: 'none'
                });

                $('.mobile-phone-select-wrapper').css({
                    display: 'block'
                });

                $('.outer-phone-holder').css({
                    marginBottom: '10px',
                    marginTop: '1px',
                    padding: '7px 0 7px 5px',
                    backgroundColor: '#eee5'
                })

                function initializeCityOptionsSelect() {
                    let city = $.cookie('ip_city') ? $.cookie('ip_city') : 'Москва';

                    $(".test-select").val(city);

                    $('.test-select').selectpicker({
                        showTick: true,
                        liveSearch: true,
                        style: 'btn-light',
                        styleBase: 'custom-select-class',
                        width: '100%'
                    });

                    function insertCityPhoneNumbber(num, formattedNum) {
                        let link = "<a href='tel:" + formattedNum + "' style='border-bottom: none;'>" + num + "</a>";
                        $('.inner-phone-holder').html(link);
                    }

                    $('.test-select').on('changed.bs.select', function(e, clickedIndex, isSelected, previousValue) {
                        let cityname = e.target[clickedIndex].value;
                        let cityNumber = e.target[clickedIndex].dataset.phone;
                        let cityFormattedNumber = e.target[clickedIndex].dataset.formattedPhone;

                        $.cookie('ip_city', null, {
                            path: '/',
                            expires: -1
                        });

                        $.cookie('ip_city', cityname, {
                            path: '/',
                            expires: 365
                        });

                        insertCityPhoneNumbber(cityNumber, cityFormattedNumber);
                    });

                    $('.test-select').on('loaded.bs.select', function(e, clickedIndex, isSelected, previousValue) {
                        let optionIndex = e.target.selectedIndex;
                        let cityNumber = $('.test-select option').eq(optionIndex).data('phone');
                        let cityFormattedNumber = $('.test-select option').eq(optionIndex).data('formatted-phone');
                        insertCityPhoneNumbber(cityNumber, cityFormattedNumber);
                    });
                }

                initializeCityOptionsSelect();
            } else {
                $('.mobile-phone-select-wrapper').css({
                    display: 'none'
                });

                // определение города пользователя без ymaps -s
                if ($.cookie('ip_city') == undefined) {

                    function returnIp(callback) {
                        $.ajax({
                            type: 'POST',
                            url: '/ajax/get_ip.php',
                            success: function(data) {
                                var ip_city = data;

                                if (callback && typeof(callback) === "function") {
                                    callback(ip_city);
                                }
                            }
                        });
                    }

                    function callbackFun(city) {

                        var city_arr = {
                            'Saint Petersburg City': 'Санкт-Петербург',
                            'Saint Petersburg': 'Санкт-Петербург',
                            'Moscow City': 'Москва',
                            'Moscow': 'Москва',
                            'Moskva': 'Москва',
                            'Novosibirsk': 'Новосибирск',
                            'Samara': 'Самара',
                            'Voronezh': 'Воронеж',
                            'Kaliningrad': 'Калининград',
                            'Rostov': 'Ростов-на-Дону',
                            'Sverdlovsk': 'Екатеринбург',
                            'Tatarstan': 'Казань',
                            'Nizhegorod': 'Нижний Новгород',
                            'Volgograd': 'Волгоград',
                            'Omsk': 'Омск',
                            'Perm\'': 'Пермь',
                            'Krasnoyarsk': 'Красноярск',
                            'Chelyabinsk': 'Челябинск',
                            'Bashkortostan': 'Уфа'
                        };

                        var city = city_arr[city];
                        var city_tel;

                        if (city == undefined) {
                            city = 'Москва';
                            city_tel = '<a href="tel:+74951343834">+7 (495) 134-38-34</a>';
                        }

                        var city_select = $(".chosen-select");
                        var select_arr = city_select[0];

                        $.each(select_arr, function(i, val) {
                            if (city == val.label) {
                                city = val.label;
                                city_tel = "<a href='tel:" + val.dataset.formattedPhone + "'>" + val.dataset.phone + "</a>";
                                return false;
                            }
                        });

                        $(".chosen-select").val(city);
                        $(".chosen-single span").html(city);
                        $(".city_inp").val(city);
                        $('.phone').html(city_tel);
                        $.cookie('ip_city', null, {
                            path: '/',
                            expires: -1
                        });
                        $.cookie('ip_city', city, {
                            path: '/',
                            expires: 365
                        });

                        $(function() {
                            $(".chosen-select").chosen({
                                no_results_text: "Город не найден",
                                disable_search: true

                            }).change(function() {
                                var town = $(this).val();
                                // console.log(town);
                                var phoneTown = $(this).find('option:selected').data('phone');
                                // console.log(phoneTown);
                                // var city_tel = "<a href='tel:'" +phoneTown+ "'>" +phoneTown+ "</a>";

                                $('.phone').html(phoneTown);
                                $.cookie('ip_city', null, {
                                    path: '/',
                                    expires: -1
                                });
                                $.cookie('ip_city', town, {
                                    path: '/',
                                    expires: 365
                                });

                                $(".city_inp").val(town);
                            });
                        });
                    }

                    returnIp(callbackFun);
                } else {
                    var city_select = $(".chosen-select");
                    var city_arr = city_select[0];
                    var city_val;
                    var city_tel;

                    $.each(city_arr, function(i, val) {
                        if ($.cookie('ip_city') == val.label) {
                            city_val = val.label;
                            city_tel = "<a href='tel:" + val.dataset.formattedPhone + "'>" + val.dataset.phone + "</a>";
                        }
                    });

                    $(".chosen-select").val(city_val);
                    $(".chosen-single span").html(city_val);
                    $(".city_inp").val(city_val);
                    $('.phone').html(city_tel);

                    // Вывод телефонов и города 
                    $(function() {
                        $(".chosen-select").chosen({
                            no_results_text: "Город не найден",
                            // disable_search: true
                        }).change(function() {
                            var town = $(this).val();
                            var phoneTown = $(this).find('option:selected').data('phone');
                            var formattedPhoneNumber = $(this).find('option:selected').data('formatted-phone');
                            var city_tel = "<a href='tel:" + formattedPhoneNumber + "'>" + phoneTown + "</a>";
                            if (town == null) {
                                $('.chosen-select').val('Москва');
                                $('.chosen-select').trigger("chosen:updated");
                                $('.chosen-select').trigger("change");
                            } else {
                                $('.phone').html(city_tel);
                                $(".city_inp").val(town);
                                $.cookie('ip_city', null, {
                                    path: '/',
                                    expires: -1
                                });
                                $.cookie('ip_city', town, {
                                    path: '/',
                                    expires: 365
                                });
                            }
                        });
                    });
                }
                // определение города пользователя без ymaps -f

            }

            // --Проверка coockies--
            getHtmlUserSelectedDomainNameWithInfoAndPrice();
            checkCountDomainCoockie();
            var current_url = window.location.pathname;
            $("[name = 'request']").val(current_url);
        });
    </script>
