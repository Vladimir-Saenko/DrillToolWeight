              window.onload = function() {document.querySelector('.menu_ttl_num0').innerHTML = '<a style=\'font-size: 17px;\' href=\'/services\'>Услуги</a>';document.querySelector('.menu_ttl_num1').innerHTML = '<a style=\'font-size: 17px;\' href=\'/about\'>Компания</a>';document.querySelector('.menu_ttl_num2').innerHTML = '<a style=\'font-size: 17px;\' href=\'/special_offers\'>Спецпредложения</a>';document.querySelector('.menu_ttl_num3').innerHTML = '<a style=\'font-size: 17px;\' href=\'/usefull_information\'>Справочная информация</a>';};</script><div class="row"><div class="col-lg-3  col-sm-6 col-md-3 col-xs-12"><div class="menu-element"><div class="row">
