            <span :class="['price product__price-current', {'price--new': hasDiscount(card)}, classNamePriceCurrent]">
                {{ price() | formatPrice }} <span class="rub">б</span>
            </span>
            <span v-if="card.isCreditable" class="product__credit page-catalog__credit">
                от {{ card.creditablePriceFrom | formatPrice }}<span class='rub'>б</span>&nbsp;/ мес.
            </span>
            <br>
            <span :class="['price price--old product__price-old', classNamePriceOld]" v-if="hasDiscount(card)">
                {{ priceWithoutDiscount() | formatPrice }} <span class="rub">б</span>
            </span>
            <br class="product__br" v-if="hasDiscount(card) && showEconomy(card)">
            <span
                :class="['price-dif product__price-dif product__discount--hide', classNamePriceDif]"
                v-if="hasDiscount(card) && showEconomy(card)"
            >
                {{ discountAmount() | formatPrice }}&nbsp;<span class="rub">б</span> чистой экономии!
            </span>
        </div>

        <div :class="['product__btn-wrapper', {'product__btn-wrapper--mixed': card.basketQuantity > 0, 'product__btn-wrapper--list': !card.isOnlyHypermarket}]">
            <div :class="['btn-mixed', { 'is-active': card.basketQuantity }]">
                <a v-if="card.isOnlyHypermarket || (card.overallStock == 0 && card.ecomFilialStock == 0)" :class="['btn-mixed__link is-disabled']" href="javascript:;" :title="card.overallStock == 0 ? 'Нет в наличии' : 'Товар доступен только в гипермаркетах'" :style="[card.overallStock != 0 ? {'background': '#6aa76d'} : '']">
                    <template v-if="card.overallStock == 0 && card.ecomFilialStock == 0">
                        Нет в наличии
                    </template>
                    <template v-else>
                        Только в гипермаркетах
                    </template>
                </a>
                <template v-else>
                    <button
                        :class="['btn-mixed__link product__btn-text', {'btn-mixed--rounded': !(card.basketQuantity > 0), 'product__btn--mixed': card.basketQuantity > 0},
                            {'is-disabled': card.overallStock <= card.basketQuantity && card.ecomFilialStock <= card.basketQuantity}]"
                        :title="card.basketQuantity < 1 && 'Добавить в корзину'"
                        @click="() => addToBasket(card)"
                    >
                        {{ card.basketQuantity == 0 ? 'В корзину' : `В корзине: ${card.basketQuantity}` }}
                    </button>
                    <button class="btn-mixed__add btn-mixed__add--rounded--right product__btn-text" type="button"
                        v-if="card.basketQuantity > 0 && (card.overallStock > card.basketQuantity || card.ecomFilialStock > card.basketQuantity)"
                        @click="card.overallStock > card.basketQuantity && addToBasket(card)"
                    >
                        +{{ card.basket_step > 1 ? card.basket_step : '' }}
                    </button>
                </template>
            </div>

            <div class="product__availability">
                <svg class="icon icon--tick-thin product__availability-icon">
                    <use xlink:href="#icon-tick_thin"></use>
                </svg>
                <span class="product__availability-text">В&nbsp;наличии в&nbsp;<strong>{{ card.availableShops | pluralize('магазине|магазинах|магазинах') }}</strong></span>
            </div>
        </div>
    </div>
</div>
            </vue-product-card>
        </li>
        </ul>

            </div>

                    <div class="carousel-pagination  carousel-pagination--mobile-only ">
            <button class="carousel-pagination__switch carousel__switch swiper-button-prev" type="button">
                <svg class="icon icon--arrow-slide-left"><use xlink:href="#icon-arrow_slide_left"></use></svg>
            </button>

            <div class="carousel-pagination__container swiper-pagination"></div>

            <button class="carousel-pagination__switch carousel__switch swiper-button-next" type="button">
                <svg class="icon icon--arrow-slide-right"><use xlink:href="#icon-arrow_slide_right"></use></svg>
            </button>
        </div>
            </div>
</vue-product-slider>
