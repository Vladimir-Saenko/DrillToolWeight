                    bitrix-method="hits"
                    gtm-List-Name="hits-main"
                    bitrix-name="product:slider"
                    :bitrix-params='{"PRODUCT_COUNT":9,"SORT_COLUMN":"ID","SORT_DIRECTION":"desc","CACHE_TIME":1200,"IMAGE_WIDTH":400,"IMAGE_HEIGHT":400,"CUSTOM_BLOCK_ID":0}'>
    <div class="carousel--products carousel-products homepage__promo"
      data-navigation       data-pagination="fraction"           :class="{carousel: !!carousel}"
     >
