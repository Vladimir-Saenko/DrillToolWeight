                {{ card.name }}
            </a>
        </p>

        <div :class="['product__compare', classNameCompare]">
            <button :class="['product-control product__control', classNameBtnCompare]" type="button"
                    @click="() => addToCompareList(card)"
                    v-if="!card.isInCompareList">
                <svg class="icon icon--compare-sm product-control__icon">
                    <use xlink:href="#icon-compare_sm"></use>
                </svg>
                <span class="product-control__text">Сравнить</span>
            </button>

            <button :class="['product-control product__control', classNameBtnCompare]" type="button"
                    @click="() => deleteFromCompareList(card)"
                    v-else-if="comparePage" title="Удалить из сравнения">
                <svg class="icon icon--cross-md product-control__icon">
                    <use xlink:href="#icon-cross_md"></use>
                </svg>
                <span class="product-control__text">Не сравнивать</span>
            </button>

            <button :class="['product-control product__control', classNameBtnCompare]" type="button"
                    @click="() => deleteFromCompareList(card)"
                    v-else="comparePage" title="Удалить из сравнения">
                <svg class="icon icon--compare-sm icon--compare-sm-green product-control__icon">
                    <use xlink:href="#icon-compare_sm"></use>
                </svg>
                <span class="product-control__text">В сравнении</span>
            </button>

            <template v-if="!comparePage">
                <button
                    :class="['product-control product-control--list-view product__control product-control--tablet-view', classNameBtnFavorites]"
                    type="button"
                    @click="() => addToFavorites(card)"
                    v-if="!card.isInFavoriteList"
                >
                    <svg class="icon icon--heart-sm product-control__icon">
                        <use xlink:href="#icon-heart_sm"></use>
                    </svg>
                    <span class="product-control__text">В&nbsp;избранное</span>
                </button>

                <button
                    :class="['product-control product-control--list-view product__control product-control--tablet-view', classNameBtnFavorites]"
                    type="button"
                    title="В избранном"
                    @click="() => deleteFromFavorites(card)"
                    v-else-if="!favoritePage"
                >
                    <svg class="icon icon--heart-sm icon--heart-sm-green product-control__icon">
                        <use xlink:href="#icon-heart_sm"></use>
                    </svg>
                    <span class="product-control__text">В избранном</span>
                </button>

                <button
                    :class="['product-control product-control--list-view product__control product-control--tablet-view', classNameBtnFavorites]"
                    type="button"
                    title="Удалить из избранного"
                    @click="() => deleteFromFavorites(card)"
                    v-else
                >
                    <svg class="icon icon--cross-md product-control__icon">
                        <use xlink:href="#icon-cross_md"></use>
                    </svg>
                    <span class="product-control__text">Удалить из избранного</span>
