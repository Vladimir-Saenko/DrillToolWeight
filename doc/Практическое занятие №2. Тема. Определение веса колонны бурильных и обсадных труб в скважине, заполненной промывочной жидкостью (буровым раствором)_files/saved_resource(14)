                <svg class="carousel-title__arrow icon icon--arrow-slide-right"><use xlink:href="#icon-arrow_slide_right"></use></svg>
            </button>
        </div>
    </div>
    
    <div class="swiper-container carousel-products__container carousel-anticontainer">
        <ul class="swiper-wrapper">
            <li class="swiper-slide carousel-products__list-item" v-for="(card, index) in cards" :key="card.id">
            <vue-product-card
                inline-template
                :card="card"
                :position="index + 1"
                :gtm-list-name="gtmListName"
            >
                <div class="product" :class="[{'product--catalog-padding': isCatalogPage, 'is-disabled': (card.overallStock == 0 && card.ecomFilialStock == 0 &&  favoritePage)}, className]">
    <div class="product__quick-view" :class="classNameQuickView">
        <button class="link product__quick-view-btn" type="button" @click="quickView">Быстрый просмотр</button>
    </div>

    <div class="product__head" :class="classNameHead">
        <a class="product__img-link" :class="classNameImgLink" :href="card.url" title="Перейти на страницу товара">
            <template v-if="enablePhotosSwiper && photosAll.length > 1">
                <v-swiper
                    :options="swiperOptions"
                    :pagination="true"
                    :max-photos-count="maxSwiperPhotos"
                    :mouse-move-swipe="true"
                    class="product-swiper"
                    controls-class="product-swiper__control"
                    pagination-class="product-swiper__bullets"
                >
                    <template v-for="(photo, index) in photosAll">
                        <li v-if="photo" class="swiper-slide" :key="index">
                            <picture>
                                <source type="image/webp" :data-srcset="photo.webp"  :srcset="photo.webp">
                                <img data-sizes="auto" class="product__img lazyload" :alt="card.name" :data-src="photo.path">
                            </picture>
                        </li>
                    </template>
                </v-swiper>
            </template>
            <template v-else>
                <picture v-if="card.catalogPhotos">
                    <source type="image/webp" :srcset="card.catalogPhotos[0].webp">
                    <img sizes="auto" class="product__img lazyload" :alt="card.name" :src="card.catalogPhotos[0].path" :set="card.catalogPhotos[0].webp">
                </picture>
            </template>
        </a>

        <div :class="['product__nameplates', classNameNameplates]">
            <template v-for="(roundTag, index) in roundTags">
                <template v-if="index < 3">
                    <span v-if="roundTag.type !== 'pictogram'"
                          class="nameplate product__nameplates-item"
                          :style="{'background-color': roundTag.textBackgroundColor }"
                          :class="roundTag.textBackgroundClass">{{ roundTag.text }}</span>
                    <span v-else
                          class="nameplate nameplate--pic product__nameplates-item"
                          :style="{'background-color': roundTag.textBackgroundColor }">
                        <img class="background__img nameplate__img" :src="roundTag.pictogram" :alt="roundTag.text">
                    </span>
                </template>
            </template>

            <div class="nameplate nameplate--long product__nameplates-long" v-if="card.credit > 0">
                <p class="nameplate__desc">в&nbsp;кредит</p>
                <p class="nameplate__value"><strong>от&nbsp;{{ card.credit }}&nbsp;<span class="rub">б</span></strong>&nbsp;/
                    мес.</p>
            </div>
        </div>
    </div>

    <div :class="['product__info product__info--clamp product__info--list-view', classNameInfo]">
