    <div class="container">
    <h3 class="homepage-section__title">Популярное</h3>
    <vue-thematic-block inline-template source="/api/internal/component/product:thematic.links/call/getLinks?params%5BLINKS_COUNT%5D=10&amp;params%5BCACHE_TIME%5D=86400">
        <ul class="category-grid js-vue-thematic-block" v-cloak>
            <li class="category-grid__item" v-for="card in cards"
                :class="{'category-grid__item--sm': card.isSmall}"
                :style="{'background-color': '#'+card.color}"
            >
                <picture class="" v-if="getAvailableWidth() < 767 && card.mobilePhoto">
                    <source v-if="card.mobilePhoto.webp"
                            type="image/webp"
                            data-sizes="auto"
                            :data-srcset="card.mobilePhoto.webp"
                            :srcset="card.mobilePhoto.webp"/>
                    <img data-sizes="auto" :data-srcset="card.mobilePhoto.path" class="category-grid__item-pic lazyload"
                         :alt="card.name" :style="card.mobileInlineCss">
                </picture>
                <picture class="" v-else>
                    <source type="image/webp"
                            data-sizes="auto"
                            :data-srcset="card.photo.webp"
                            :srcset="card.photo.webp"/>
                    <img data-sizes="auto" :data-srcset="card.photo.path" class="category-grid__item-pic lazyload"
                         :alt="card.name" :style="card.inlineCss">
                </picture>
                <a class="category-grid__item-link" :href="card.link" :title="card.linkText"></a>
                <p class="category-grid__item-name">{{ card.name }}</p>

                <vue-product-card inline-template :card="card.product" :parent="card">
                    <div class="category-grid__item-footer">
                        <template v-if="card">
                            <p class="category-grid__item-product">
                                <a class="link link--black" :href="card.url" :title="card.name">{{ card.name }}</a>
                            </p>
                            <p class="category-grid__item-price">{{ (card.discountPrice ? card.discountPrice : card.price) | formatPrice }} <span class="rub">б</span></p>
                        </template>

                        <div class="category-grid__item-btns">
                            <div class="category-grid__item-basket">
                                <div class="btn-mixed" :class="{ 'is-active': card.basketQuantity }" v-if="card">
                                    <a class="btn-mixed__link" href="javascript:;" title="Добавить в корзину" v-if="card.overallStock > card.basketQuantity" @click="addToBasket(card)">
                                        <template v-if="card.overallStock == 0">
                                            Нет в наличии
                                        </template>
                                        <template v-else-if="card.basketQuantity == 0">
                                            В корзину
                                        </template>
                                        <template v-else>
                                            В корзине: {{ card.basketQuantity }}
                                        </template>
                                    </a>
                                    <a class="btn-mixed__link is-disabled" href="javascript:;" title="Добавить в корзину" v-if="card.overallStock <= card.basketQuantity">
                                        <template v-if="card.overallStock == 0">
                                            Нет в наличии
                                        </template>
                                        <template v-else>
                                            В корзине: {{ card.basketQuantity }}
                                        </template>
                                    </a>
                                    <button class="btn-mixed__add" type="button" v-if="card.overallStock > card.basketQuantity" @click="addToBasket(card)">+{{ card.basket_step }}</button>
                                </div>
                            </div>
                            <a class="category-grid__item-all btn" :href="parent.link">{{ parent.linkText }}</a>
                        </div>
                    </div>
                </vue-product-card>
            </li>
        </ul>
    </vue-thematic-block>
</div>
